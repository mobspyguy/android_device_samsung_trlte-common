# Services begin here

service adsprpcd /system/vendor/bin/adsprpcd
    class main
    user media
    group media

service qcamerasvr /system/vendor/bin/mm-qcamera-daemon
    class main
    user camera
    group camera system inet input graphics bluetooth

service qseecomd /system/vendor/bin/qseecomd
    class core
    user root
    group root

service mpdecision /system/vendor/bin/mpdecision --avg_comp
    class main
    user root
    group root readproc

service sensorhubservice /system/vendor/bin/sensorhubservice
    class main
    user system
    group input

service thermal-engine /system/vendor/bin/thermal-engine
    class main
    user root
    socket thermal-send-client stream 0666 system system
    socket thermal-recv-client stream 0660 system system
    socket thermal-recv-passive-client stream 0666 system system
    group root

service timekeep /system/vendor/bin/timekeep restore
    class late_start
    user root
    group root
    oneshot

service irsc_util /system/vendor/bin/irsc_util "/system/vendor/etc/sec_config"
    class main
    user root
    oneshot

service rmt_storage /system/vendor/bin/rmt_storage
    class core
    user root
    disabled

service qmuxd /system/vendor/bin/qmuxd
    class core
    user root
    group radio audio bluetooth gps diag log

service mdm_helper /system/vendor/bin/mdm_helper
    class core
    onrestart setprop ro.service.mdm_helper_restarted "true"
    group root system radio oem_2950 wakelock

service mdm_helper_proxy /system/vendor/bin/mdm_helper_proxy
    class core
    user system
    group system wakelock
    disabled

on property:init.svc.ril-daemon=running
    setprop ro.mdm_helper_proxy_req true

    # Start mdm_helper_proxy
on property:ro.mdm_helper_proxy_req=true
    start mdm_helper_proxy

    # Stop mdm_helper_proxy in case of shutdown
on property:sys.shutdown.requested=*
    stop mdm_helper_proxy

service wpa_supplicant /vendor/bin/hw/wpa_supplicant \
    -O/data/vendor/wifi/wpa/sockets -puse_p2p_group_interface=1 \
    -g@android:wpa_wlan0
    interface android.hardware.wifi.supplicant@1.0::ISupplicant default
    interface android.hardware.wifi.supplicant@1.1::ISupplicant default
    class main
    socket wpa_wlan0 dgram 660 wifi wifi
    disabled
    oneshot

service olsrd /system/vendor/bin/olsrd -f /system/vendor/etc/wifi/olsrd.conf
    class main
    disabled
    oneshot

service loc_launcher /system/vendor/bin/loc_launcher
    #loc_launcher will start as root and set its uid to gps
    class late_start
    group gps inet net_raw diag net_admin wifi

service macloader /system/vendor/bin/macloader
    class late_start
    oneshot
    seclabel u:r:macloader:s0

# Run deferred initcalls of builtin kernel modules (bcmdhd)
service wifiloader /system/bin/wifiloader
     class main
     oneshot
     seclabel u:r:wifiloader:s0

on property:init.svc.macloader=stopped
    chown system root /data/.cid.info
    chmod 0664 /data/.cid.info
    chown system root /data/.rev
    chmod 0664 /data/.rev

on property:crypto.driver.load=1
    insmod /system/lib/modules/qce.ko
    insmod /system/lib/modules/qcedev.ko

on property:crypto.driver.load=0
    exec /system/bin/rmmod qcedev.ko
    exec /system/bin/rmmod qce.ko

on property:sys.boot_completed=1
    write /sys/block/mmcblk0/queue/scheduler cfq

    # Enable ZRAM on boot_completed
    swapon_all fstab.qcom

    # Permissions for S-Pen
    chmod 0660 /dev/input/event18
    chmod 0660 /dev/input/event19
    chown system input /dev/input/event18
    chown system input /dev/input/event19
    chmod 0660 /sys/class/input/input18/enabled
    chown system system /sys/class/input/input18/enabled
    chown system radio /sys/class/sec/sec_epen/epen_firm_update
    chown system radio /sys/class/sec/sec_epen/epen_reset
    chown system radio /sys/class/sec/sec_epen/epen_reset_result
    chown system radio /sys/class/sec/sec_epen/epen_checksum
    chown system radio /sys/class/sec/sec_epen/epen_checksum_result
    chown system radio /sys/class/sec/sec_epen/epen_saving_mode
    chown system radio /sys/class/sec/sec_epen/epen_connection
    chown system radio /sys/class/sec/sec_epen/epen_tuning_version
    chown system radio /sys/class/sec/sec_epen/epen_firm_update_status
    chown system radio /sys/class/sec/sec_epen/epen_firm_version
    chown system radio /sys/class/sec/sec_epen/epen_delay_time
    chown system radio /sys/class/sec/sec_epen/boost_level

    # Set idle GPU to 133 Mhz
    write /sys/class/kgsl/kgsl-3d0/default_pwrlevel 7

    #Audience ES705 UART
    # Late chown to make sure the kernel can access the device
    chown media audio /dev/ttyHS3

    # Symlink directories to access telephony.db and preferred-apn.xml required by libsec-ril
    symlink /data/user_de/0/com.android.providers.telephony/databases /data/data/com.android.providers.telephony/databases
    symlink /data/user_de/0/com.android.providers.telephony/shared_prefs /data/data/com.android.providers.telephony/shared_prefs

on property:ro.data.large_tcp_window_size=true
    # Adjust socket buffer to enlarge TCP receive window for high bandwidth (e.g. DO-RevB)
    write /proc/sys/net/ipv4/tcp_adv_win_scale  1

service charger /charger
    class charger
    seclabel u:r:charger:s0

service sec-sh /system/vendor/bin/sh /system/vendor/etc/init.sec.boot.sh
    class main
    user root
    oneshot

